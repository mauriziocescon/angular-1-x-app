{
  "compileOnSave": false,
  "compilerOptions": {
    "allowJs": true,
    "diagnostics": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "CommonJS",
    "moduleResolution": "Node",
    "noEmitOnError": false,
    "noFallthroughCasesInSwitch": false,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noImplicitThis": false,
    "pretty": true,
    "removeComments": false,
    "target": "ES2015",
    "types": ["jasmine", "service_worker_api"]
  },
  "exclude": [
    "node_modules",
    "src"
  ]
}