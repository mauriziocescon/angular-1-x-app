{
  "name": "angular-1-x-app",
  "version": "1.0.0",
  "description": "A demo application built on angular 1.x components, Typescript, ES2017, ui-router",
  "author": "Maurizio Cescon <maurizio.cescon@gmail.com> (http://www.mauriziocescon.com)",
  "homepage": "http://www.mauriziocescon.com",
  "private": false,
  "engines": {
    "node": ">=6.11.0",
    "npm": ">=3.10.10"
  },
  "scripts": {
    "postinstall": "gulp prod --mock=false --strictDi=true",
    "prestart": "gulp prod --mock=false --strictDi=true",
    "start": "http-server ./dist -a localhost -p 8000 -o -c-1 -i false",
    "serve": "gulp default --mock=false --strictDi=false",
    "serve-mock": "gulp default --mock=true --strictDi=false",
    "build": "gulp prod --mock=false --strictDi=true",
    "build-mock": "gulp prod --mock=true --strictDi=true",
    "pretest": "gulp test --mock=false --strictDi=true",
    "test": "karma start karma.conf.js",
    "preprotractor": "tsc --project e2e-tests && webdriver-manager update",
    "protractor": "protractor protractor.conf.js",
    "test-api": "jasmine-node rest-api-tests/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mauriziocescon/angular-1-x-app"
  },
  "keywords": [
    "angular",
    "bootstrap",
    "browser-sync",
    "browserify",
    "cescon",
    "es2017",
    "gulp",
    "maurizio",
    "sass",
    "typescript",
    "vendor",
    "watchify"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/brentertz/scapegoat/blob/master/LICENSE-MIT"
    }
  ],
